class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {
        for(int i=0;i<nums.length-1;i++){
            int num = nums[i];
            for(int j = i+1;j<nums.length;j++){
                if(nums[j]== num && Math.abs(j-i)<=k)
                    return true;
            }
        }
        return false;
    }
}
