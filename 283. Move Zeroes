class Solution {
    public void moveZeroes(int[] nums) {
        int count = 0;
        int n = nums.length;
        for(int i=0;i<n;i++){
            if(nums[i] == 0){
                count++;
            }
            else{
                nums[i - count] = nums[i];
            }
        }
        
        for(int i=n-1;i>n-1-count;i--){
            nums[i] = 0;
        }
    }
}
